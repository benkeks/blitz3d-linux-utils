    <contexts>
      <context attribute="Normal Text" lineEndContext="#stay" name="Normal">

        <RegExpr attribute="Slopes" context="#stay" String="\b(for)([\s]|$)" insensitive="true" beginRegion="ForRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="\b(next)([\s]|$)" insensitive="true" endRegion="ForRegion"/>        
        
        <RegExpr attribute="Branches" context="#stay" String="\b(then)\s+" insensitive="true" endRegion="IfRegion" />
        
        <RegExpr attribute="Branches" context="#stay" String="^(\s*)?if" insensitive="true" beginRegion="IfRegion" /> 
        <RegExpr attribute="Branches" context="#stay" String="\b((end if)|(endif))([\s]|$)" insensitive="true" endRegion="IfRegion"/>   
        
        
        <RegExpr attribute="Branches" context="#stay" String="\b(select)([\s]|$)" insensitive="true" beginRegion="SelectRegion"/>
        <RegExpr attribute="Branches" context="#stay" String="\b(end select)([\s]|$)" insensitive="true" endRegion="SelectRegion"/>
        
        
        
        <RegExpr attribute="Data Types" context="#stay" String="\b(type)([\s]|$)" insensitive="true" beginRegion="StructureRegion"/>
        <RegExpr attribute="Data Types" context="#stay" String="\b(end type)([\s]|$)" insensitive="true" endRegion="StructureRegion"/>
        
        <RegExpr attribute="Data Types" context="#stay" String="\b(with)([\s]|$)" insensitive="true" beginRegion="WithRegion"/>
        <RegExpr attribute="Data Types" context="#stay" String="\b(end with)([\s]|$)" insensitive="true" endRegion="WithRegion"/>
        
         
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(while)([\s]|$)" insensitive="true" beginRegion="WhileRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(wend)([\s]|$)" insensitive="true" endRegion="WhileRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(repeat)([\s]|$)" insensitive="true" beginRegion="RepeatRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(until)([\s]|$)" insensitive="true" endRegion="RepeatRegion"/>
        

        <RegExpr attribute="Data Types" context="#stay" String="\b(for (input|output|binary|random))([\s]|$)" insensitive="true" />

        <keyword attribute="Keyword" context="#stay" String="keywords"/>
        <keyword attribute="Data Types" context="#stay" String="Data Types"/>

        <keyword attribute="Functions" context="#stay" String="functions"/>

        <RegExpr attribute="Constant" context="#stay" String="\#+[a-zA-Z_\x7f-\xff][a-zA-Z0-9_\x7f-\xff]*"/>
        <Float attribute="Number" context="#stay"/>

        <DetectChar attribute="String" context="String" char="&quot;" />
        
        <RegExpr attribute="Region Marker" context="#stay" String="^\s*\s*FUNCTION.*$" beginRegion="marker"/>
        <RegExpr attribute="Region Marker" context="#stay" String="^\s*\s*END\ FUNCTION.*$" endRegion="marker"/>
        
        <DetectChar attribute="Comment" context="Comment1" char=";"/>
        <RegExpr attribute="Branches" context="#stay" String="end\ if" insensitive="true" />

        <RegExpr attribute="Keyword" context="#stay" String="for\ input" insensitive="true" />
        <RegExpr attribute="Keyword" context="#stay" String="for\ output" insensitive="true" />
        <RegExpr attribute="Keyword" context="#stay" String="for\ binary" insensitive="true" />
        <RegExpr attribute="Keyword" context="#stay" String="for\ append" insensitive="true" />
        <RegExpr attribute="Keyword" context="#stay" String="for\ random" insensitive="true" />

        <RegExpr attribute="Comment"  context="#stay" String="^(\s?)*[0-9]*\s+" insensitive="true" />

        <RegExpr attribute="Number"  context="#stay" String="\b\d+\b" insensitive="true" />


        <AnyChar attribute="Other" context="#stay" String=";^()}{:,/[]+-*\=&lt;&gt;" />
        <keyword attribute="Slopes" context="#stay" String="Slopes"/>
        <keyword attribute="Branches" context="#stay" String="Branches"/>
      </context>


      <context attribute="String" lineEndContext="#pop" name="String">
        <DetectChar attribute="String" context="#pop" char="&quot;"/>
      </context>

      <context attribute="Comment" lineEndContext="#pop" name="Comment1">
        <keyword attribute="Number" context="#stay" String="attention" />
      </context>

    </contexts>

